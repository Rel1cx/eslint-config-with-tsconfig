{
  "name": "eslint-config-with-tsconfig",
  "version": "2.9.220",
  "description": "Rel1cx's ESLint config",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./tsconfig.base.json": "./tsconfig.base.json"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "tsconfig.base.json"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf lib node_modules bun.lockb",
    "fix:ts": "eslint --fix .",
    "format:check": "dprint check",
    "format:write": "dprint fmt",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:type": "tsc --noEmit",
    "publish": "bun run build && pnpm publint",
    "update": "taze"
  },
  "dependencies": {
    "@babel/core": "7.23.6",
    "@babel/eslint-parser": "7.23.3",
    "@babel/preset-react": "7.23.3",
    "@tsconfig/node20": "20.1.2",
    "@tsconfig/strictest": "2.0.2",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "eslint-config-rel1cx": "0.12.220",
    "eslint-define-config": "2.0.0",
    "eslint-plugin-expect-type": "0.3.0",
    "eslint-plugin-import-access": "2.1.2",
    "eslint-plugin-perfectionist": "1.5.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "eslint-plugin-regexp": "2.1.2",
    "eslint-plugin-rxjs": "5.0.3",
    "eslint-plugin-security": "2.1.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "eslint-plugin-unicorn": "49.0.0",
    "get-tsconfig": "4.7.2"
  },
  "devDependencies": {
    "@types/eslint": "8.56.0",
    "@types/node": "20.10.5",
    "dprint": "0.44.0",
    "eslint": ">=8.56.0",
    "publint": "0.2.6",
    "taze": "0.13.0",
    "tsup": "8.0.1",
    "type-fest": "4.8.3",
    "typescript": ">=5.3.3"
  },
  "peerDependencies": {
    "eslint": ">=8.56.0",
    "typescript": ">=5.3.3"
  },
  "packageManager": "pnpm@8.12.1",
  "engines": {
    "bun": ">=1.0.13",
    "node": ">=18.15.0"
  }
}
