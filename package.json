{
    "name": "eslint-config-with-tsconfig",
    "version": "2.6.1",
    "description": "Rel1cx's ESLint config",
    "exports": {
        ".": {
            "types": "./lib/index.d.ts",
            "require": "./lib/index.js"
        },
        "./prettier": {
            "types": "./lib/prettier.d.ts",
            "require": "./lib/prettier.js"
        },
        "./tsconfig.base.json": "./tsconfig.base.json"
    },
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "files": [
        "lib",
        "tsconfig.base.json"
    ],
    "scripts": {
        "build": "tsup",
        "clean": "rimraf lib node_modules pnpm-lock.yaml",
        "fix:ts": "pnpm eslint --fix .",
        "format:check": "prettier -u .",
        "format:write": "prettier -w -u .",
        "lint": "pnpm eslint .",
        "publish": "pnpm run build && pnpm publint",
        "typecheck": "pnpm tsc --noEmit"
    },
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "^6.6.0",
        "@typescript-eslint/parser": "^6.6.0",
        "consola": "^3.2.3",
        "eslint-config-relicx": "^0.12.0",
        "eslint-define-config": "^1.23.0",
        "eslint-plugin-expect-type": "^0.2.3",
        "eslint-plugin-import-access": "^2.1.1",
        "get-tsconfig": "^4.7.0",
        "prettier-plugin-curly": "^0.1.2",
        "prettier-plugin-packagejson": "^2.4.5",
        "prettier-plugin-toml": "^1.0.0"
    },
    "devDependencies": {
        "@types/eslint": "8.44.2",
        "@types/node": "20.5.9",
        "concurrently": "8.2.1",
        "eslint": "8.49.0",
        "prettier": "3.0.3",
        "publint": "0.2.2",
        "tsup": "7.2.0",
        "type-fest": "4.3.1",
        "typescript": "5.2.2"
    },
    "peerDependencies": {
        "eslint": ">=8.48.0",
        "prettier": ">=3.0.3",
        "typescript": ">=5.2.2"
    },
    "engines": {
        "node": ">=18.16.0"
    }
}
