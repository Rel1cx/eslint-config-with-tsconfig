{
    "name": "eslint-config-with-tsconfig",
    "version": "1.9.8",
    "description": "Rel1cx's ESLint config",
    "exports": {
        ".": {
            "types": "./lib/index.d.ts",
            "require": "./lib/index.js"
        },
        "./helper": {
            "types": "./lib/helper.d.ts",
            "require": "./lib/helper.js"
        },
        "./tsconfig.base.json": "./tsconfig.base.json"
    },
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "files": [
        "lib",
        "tsconfig.base.json"
    ],
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf lib",
        "dev": "tsup --watch",
        "format": "rome format . --write",
        "lint": "eslint --color . --ext .ts --fix",
        "prepublish": "publint && pnpm build"
    },
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "consola": "^3.2.3",
        "eslint-config-relicx": "^0.5.0",
        "eslint-config-typed-fp": "^5.1.0",
        "eslint-define-config": "^1.21.0",
        "eslint-plugin-case-police": "^0.6.1",
        "eslint-plugin-etc": "^2.0.3",
        "eslint-plugin-expect-type": "^0.2.3",
        "eslint-plugin-import": "npm:eslint-plugin-i@latest",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "eslint-plugin-regexp": "^1.15.0",
        "eslint-plugin-rxjs": "^5.0.3",
        "eslint-plugin-security": "^1.7.1",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-sonarjs": "^0.19.0",
        "eslint-plugin-sort": "^2.10.0",
        "eslint-plugin-tailwindcss": "^3.13.0",
        "eslint-plugin-unicorn": "^47.0.0",
        "get-tsconfig": "^4.6.2"
    },
    "devDependencies": {
        "@types/eslint": "8.44.0",
        "@types/node": "20.4.2",
        "eslint": "8.45.0",
        "eslint-config-relicx": "0.5.0",
        "publint": "0.1.16",
        "rome": "12.1.3",
        "tsup": "7.1.0",
        "type-fest": "3.13.0",
        "typescript": "5.1.6"
    },
    "peerDependencies": {
        "eslint": ">=8.43.0",
        "typescript": ">=5.1.6"
    },
    "engines": {
        "node": ">=18.16.0"
    }
}
