{
    "name": "eslint-config-with-tsconfig",
    "version": "2.4.3",
    "description": "Rel1cx's ESLint config",
    "exports": {
        ".": {
            "types": "./lib/index.d.ts",
            "require": "./lib/index.js"
        },
        "./tsconfig.base.json": "./tsconfig.base.json"
    },
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "files": [
        "lib",
        "tsconfig.base.json"
    ],
    "scripts": {
        "build": "tsup",
        "format": "prettier -w -u .",
        "install:clean": "rimraf lib node_modules pnpm-lock.yaml && pnpm install",
        "lint": "pnpm eslint --fix .",
        "publish": "pnpm run build && pnpm publint",
        "typecheck": "pnpm tsc --noEmit"
    },
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "^6.2.1",
        "@typescript-eslint/parser": "^6.2.1",
        "consola": "^3.2.3",
        "eslint-config-relicx": "^0.9.2",
        "eslint-define-config": "^1.22.0",
        "eslint-plugin-expect-type": "^0.2.3",
        "eslint-plugin-import": "npm:eslint-plugin-i@latest",
        "get-tsconfig": "^4.6.2"
    },
    "devDependencies": {
        "@types/eslint": "8.44.1",
        "@types/node": "20.4.5",
        "concurrently": "8.2.0",
        "eslint": "8.46.0",
        "prettier": "3.0.0",
        "publint": "0.2.0",
        "tsup": "7.2.0",
        "type-fest": "4.1.0",
        "typescript": "5.1.6"
    },
    "peerDependencies": {
        "eslint": ">=8.46.0",
        "typescript": ">=5.1.6"
    },
    "engines": {
        "node": ">=18.16.0"
    }
}
