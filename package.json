{
  "name": "eslint-config-with-tsconfig",
  "version": "2.15.2",
  "description": "Rel1cx's ESLint config",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./tsconfig.base.json": "./tsconfig.base.json"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "tsconfig.base.json"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf lib node_modules bun.lockb",
    "fix:ts": "eslint --fix .",
    "format:check": "dprint check",
    "format:write": "dprint fmt",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:type": "tsc --noEmit",
    "publish": "bun run build && pnpm publint",
    "update": "taze"
  },
  "dependencies": {
    "@tsconfig/node20": "20.1.2",
    "@tsconfig/strictest": "2.0.3",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "eslint-config-rel1cx": "0.15.2",
    "eslint-define-config": "2.1.0",
    "eslint-plugin-expect-type": "0.3.0",
    "eslint-plugin-import-access": "2.1.2"
  },
  "devDependencies": {
    "@types/eslint": "8.56.2",
    "@types/node": "20.11.19",
    "dprint": "0.45.0",
    "eslint": ">=8.56.0",
    "publint": "0.2.7",
    "taze": "0.13.3",
    "tsup": "8.0.2",
    "type-fest": "4.10.2",
    "typescript": ">=5.3.3"
  },
  "peerDependencies": {
    "eslint": ">=8.56.0",
    "typescript": ">=5.3.3"
  },
  "packageManager": "pnpm@8.14.0",
  "engines": {
    "bun": ">=1.0.13",
    "node": ">=18.15.0"
  },
  "pnpm": {
    "overrides": {
      "hasown": "npm:@nolyfill/hasown@latest"
    }
  }
}
