{
    "name": "eslint-config-with-tsconfig",
    "version": "2.7.8",
    "description": "Rel1cx's ESLint config",
    "exports": {
        ".": {
            "import": {
                "types": "./lib/index.d.mts",
                "default": "./lib/index.mjs"
            },
            "require": {
                "types": "./lib/index.d.ts",
                "default": "./lib/index.js"
            }
        },
        "./tsconfig.base.json": "./tsconfig.base.json"
    },
    "main": "./lib/index.js",
    "module": "./lib/index.mjs",
    "types": "./lib/index.d.ts",
    "files": [
        "lib",
        "tsconfig.base.json"
    ],
    "scripts": {
        "build": "tsup",
        "update": "taze",
        "clean": "rimraf lib node_modules bun.lockb",
        "fix:ts": "eslint --fix .",
        "format:check": "dprint check",
        "format:write": "dprint fmt",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "lint:type": "tsc --noEmit",
        "publish": "bun run build && pnpm publint"
    },
    "dependencies": {
        "@babel/core": "7.23.2",
        "@babel/eslint-parser": "7.22.15",
        "@babel/preset-react": "7.22.15",
        "@typescript-eslint/eslint-plugin": "6.8.0",
        "@typescript-eslint/parser": "6.8.0",
        "consola": "3.2.3",
        "eslint-config-relicx": "0.12.18",
        "eslint-define-config": "1.24.1",
        "eslint-plugin-expect-type": "0.2.3",
        "eslint-plugin-import-access": "2.1.2",
        "eslint-plugin-perfectionist": "1.5.1",
        "eslint-plugin-promise": "6.1.1",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-react-refresh": "0.4.3",
        "eslint-plugin-regexp": "2.1.0",
        "eslint-plugin-rxjs": "5.0.3",
        "eslint-plugin-security": "1.7.1",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "eslint-plugin-sonarjs": "0.21.0",
        "eslint-plugin-tailwindcss": "3.13.0",
        "eslint-plugin-unicorn": "48.0.1",
        "get-tsconfig": "4.7.2"
    },
    "devDependencies": {
        "@types/eslint": "8.44.6",
        "@types/node": "20.8.7",
        "dprint": "0.41.0",
        "eslint": ">=8.52.0",
        "publint": "0.2.5",
        "taze": "0.11.4",
        "tsup": "7.2.0",
        "type-fest": "4.6.0",
        "typescript": ">=5.2.2"
    },
    "peerDependencies": {
        "eslint": ">=8.49.0",
        "typescript": ">=5.2.2"
    },
    "packageManager": "bun@1.0.7",
    "engines": {
        "bun": ">=1.0.3",
        "node": ">=18.16.0"
    }
}
